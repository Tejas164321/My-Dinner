
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // This rule allows any authenticated user to read and write to the database.
    // This is useful for getting started, but it's important to secure your
    // database with more specific rules before going to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
